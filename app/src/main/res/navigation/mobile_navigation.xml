<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="usa.sesion1.reto2.ui.home.HomeFragment"
        android:label="@string/menu_registro"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="usa.sesion1.reto2.ui.gallery.GalleryFragment"
        android:label="@string/menu_menus"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="usa.sesion1.reto2.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_sucursales"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/nav_menu"
        android:name="usa.sesion1.reto2.menuFragment"
        android:label="@string/menu_carrito"
        tools:layout="@layout/fragment_menu" />
</navigation>